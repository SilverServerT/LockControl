        async function initializeApp() {
            try {
                await loadConfig();
                await verifyAuth();
                
                // Initialize the rest of your application
                const state = await getLockState(false);
                
                // Set up polling interval
                setInterval(() => {
                    getLockState(true).catch(error => {
                        console.error('Background poll error:', error);
                    });
                }, 60000);

                updateInfoBar();
                setInterval(updateInfoBar, 60000);

                updateSunsetTime();
                setInterval(updateSunsetTime, 3600000);
                
                // Initialize timer if unlocked
                if (state === 'off') {
                    initializeTimer();
                }
                
                // Check sunset status and set up periodic checks
                checkSunsetStatus();
                setInterval(checkSunsetStatus, 60000);
                
            } catch (error) {
                console.error('Error initializing app:', error);
                document.getElementById('lockStatusText').textContent = 'Error initializing app';
                document.getElementById('lockStatusText').className = 'status-text';
            }
        }

        // MQTT Client Setup
        const client = new Paho.MQTT.Client(location.hostname, 9001, "lockcontrol_" + Math.random().toString(16).substr(2, 8));

        client.onConnectionLost = function(responseObject) {
            if (responseObject.errorCode !== 0) {
                console.log("Connection Lost: " + responseObject.errorMessage);
                updateLockStatus('unknown');
            }
        };

        client.onMessageArrived = function(message) {
            console.log("Message received: " + message.payloadString);
            if (message.destinationName === "stat/tasmota/POWER") {
                updateLockStatus(message.payloadString);
            }
            // Add debug message handling
            if (message.destinationName === "lockcontrol/debug") {
                console.log('Debug Info:', JSON.parse(message.payloadString));
            }
        };

        client.connect({
            onSuccess: function() {
                console.log("Connected to MQTT broker");
                client.subscribe("stat/tasmota/POWER");
                // Subscribe to debug topic
                client.subscribe("lockcontrol/debug");
                updateLockStatus('unknown');
            },
            onFailure: function(message) {
                console.log("Connection failed: " + message.errorMessage);
                updateLockStatus('unknown');
            }
        });

        // Add callService function
        async function callService(service, data) {
            try {
                // Split the service into domain and service name
                const [domain, serviceName] = service.split('.');
                console.log('Calling service:', domain, serviceName, 'with data:', data);
                
                const response = await fetch(`${config.HA_URL}/api/services/${domain}/${serviceName}`, {
                    method: 'POST',
                    headers: {
                        'Authorization': `Bearer ${config.HA_TOKEN}`,
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(data)
                });

                if (!response.ok) {
                    throw new Error(`Service call failed: ${response.status}`);
                }

                const result = await response.json();
                console.log('Service call result:', result);
                return result;
            } catch (error) {
                console.error('Error calling service:', error);
                throw error;
            }
        }

        // Add storeTimerState function
        async function storeTimerState(startTime, duration) {
            try {
                const endTime = new Date(startTime.getTime() + duration * 1000);
                const year = endTime.getFullYear();
                const month = String(endTime.getMonth() + 1).padStart(2, '0');
                const day = String(endTime.getDate()).padStart(2, '0');
                const hours = String(endTime.getHours()).padStart(2, '0');
                const minutes = String(endTime.getMinutes()).padStart(2, '0');
                const secs = String(endTime.getSeconds()).padStart(2, '0');
                
                const formattedDate = `${year}-${month}-${day} ${hours}:${minutes}:${secs}`;
                
                await callService('input_datetime.set_datetime', {
                    entity_id: config.stateEntities.unlockEnd,
                    datetime: formattedDate
                });
            } catch (error) {
                console.error('Error storing timer state:', error);
                throw error;
            }
        }

        // Add clearTimerState function
        async function clearTimerState() {
            try {
                await callService('input_datetime.set_datetime', {
                    entity_id: config.stateEntities.unlockEnd,
                    datetime: "1970-01-01 00:00:00"
                });
            } catch (error) {
                console.error('Error clearing timer state:', error);
                throw error;
            }
        }
    </script>
</body>
</html>