        async function updateSunsetTime() {
            try {
                if (!config.HA_TOKEN) {
                    console.error('No authentication token available');
                    document.getElementById('sunsetTime').textContent = '--:--';
                    document.getElementById('sunriseTime').textContent = '--:--';
